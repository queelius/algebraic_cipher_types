{
    "env": {
      "myDefaultIncludePath": ["${workspaceFolder}", "${workspaceFolder}/include"],
      "myCompilerPath": "/usr/bin/gcc-9"
    },
    "configurations": [
    {
        "name": "Win32",
        "intelliSenseMode": "gcc-9",
        "cStandard": "c11",
        "cppStandard": "c++2a",
        "includePath": [
            "${default}"
        ],
        "defines": [
            "${default}"
        ],
        "forcedInclude": [
            "${default}"
        ],
        "compileCommands": "${default}",
        "compilerPath": "${default}",
        "browse": {
            "path": ["${workspaceFolder}"],
            "limitSymbolsToIncludedHeaders": true,
            "databaseFilename": ""
            }
        }
    ],
    "version": 4
}